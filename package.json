  "bugs": {
    "url": "https://github.com/karlgroves/docraptor-client/issues"
  },
  "homepage": "https://github.com/karlgroves/docraptor-client#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.609.0",
    "aws-sdk": "^2.1655.0",
    "axios": "^1.7.2",
    "bunyan": "^1.8.15",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "prettier": "^3.3.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "pm2": "^5.4.1",
    "supertest": "^7.0.0"
